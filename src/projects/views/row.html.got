{{if not .project.Id}}
<tr>
  <td>Status</td>
  <td>Url</td>
  <td>Name</td>
  <td>Summary</td>
  <td>Updated</td>
  <td>Actions</td>
</tr>
{{else}}
<tr class="{{if odd .i}}odd{{end}}">
  <td>{{.project.StatusDisplay}}</td>
  <td><a href="/projects/{{.project.Id}}/">{{.project.Name}}</a></td>
  <td>{{.project.Summary}}</td>
  <td>{{time .project.UpdatedAt}}</td>
  <td><a href="{{.project.URLUpdate}}">Edit</a></td>
</tr>
{{end}}
